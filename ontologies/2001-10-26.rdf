        <dc:creator><wn:Person foaf:nick="AaronSw"/></dc:creator>
       </foaf:chatEvent>
      </rdf:li>
      <rdf:li>
       <foaf:chatEvent rdf:ID="T22-36-49">
        <dc:date>2001-10-26T22:36:49Z</dc:date>
